[run]
branch = true
parallel = true
omit =
     setup*
     aiosmtpd/testing/*
     aiosmtpd/tests/*
    .tox/*/lib/python3.*/site-packages/*
    .tox/*/Lib/site-packages/*
    .tox/Lib/site-packages/*
    .tox/*/site-packages/*

[paths]
source =
    aiosmtpd

[report]
exclude_lines =
    pragma: nocover
    pragma: no${PLATFORM}
    pragma: skipif_${PY_39}_py39
    pragma: skipif_${PY_36}_py36
